<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper>
    <select id="getLoginAdministrator" resultType="Administrator" parameterType="Administrator">
        select *from administrator where adminName = #{adminNamde} and adminPwd = #{adminPwd}
    </select>
</mapper>